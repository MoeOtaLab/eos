{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "output",
    "sourceMap": true,
    "useDefineForClassFields": true,
    "lib": [
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "jsx": "react-jsx"
  },
}